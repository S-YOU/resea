    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (x64)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/kernel/kernel.elf",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing"
                },
                {
                    "text": "set architecture i386:x86-64"
                }
            ],
            "miDebuggerServerAddress": "localhost:1234",
        }
    ]
}
